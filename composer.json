{
  "name": "ashrafi/laravel-wallet",
  "description": "Laravel wallet manager.",
  "keywords": ["laravel", "ui"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ramin Ashrafi",
      "email": "ashrafimanesh@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
    "illuminate/database": "^5.0|^6.0|^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Ashrafi\\WalletManager\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ashrafi\\WalletManager\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true

}
